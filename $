from day1 import read_one_value_lines

values = read_one_value_lines('ressources/day2.txt')

split_value = []
depth = 0
horizontal = 0

def go_down(change:int):
    depth += change

def go_up(change:int):
    depth -= change

def go_forward(change:int):
    horizontal += change

commands = {
            'down' : go_down(split_value[1]),
            'up' : go_up(split_value[1]),
            'forward' : go_forward(split_value[1])
        }

for value in values:
    split_value = value.split()
    commands[split_value[0]]

print('Final Position mult.: ' + str(depth*horizontal))
